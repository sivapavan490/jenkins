pipeline {
    agent {
        node{
            label 'siva'
        } 
    }
// build
    stages {
        stage('Example') {
            steps {
                echo 'Hello World'
            }
        }
         stage('Example 1') {
            steps {
                echo 'Hello World'
            }
        }
         stage('Example 2 ') {
            steps {
                echo 'Hello World'
            }
        }
    }
    // post build
    post{
        always{
            echo " pipeline is stabel"
        }
        failure{
            echo "pipeline is failure"
        }
    }
}